programa {
  funcao inicio() {

  
   real base, altura, area

    // Solicita ao usuário os valores da base e altura do paralelogramo

    escreva("Digite o valor da base do paralelogramo: ")

    leia(base)

    escreva("Digite o valor da altura do paralelogramo: ")

    leia(altura)
    
    // Calcula a área do paralelogramo

    area <- base * altura
    
    // Exibe o resultado

    escreva("A área do paralelogramo é: ", area, "\n")


    
  }
}
