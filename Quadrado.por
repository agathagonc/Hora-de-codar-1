programa {
  funcao inicio() {

   real lado, area

    // Solicita ao usuário o valor do lado do quadrado

    escreva("Digite o valor do lado do quadrado: ")

    leia(lado)
    
    // Calcula a área do quadrado (lado²)

    area <- lado * lado
    
    // Exibe o resultado
    
    escreva("A área do quadrado é: ", area, "\n")
    
  }
}
